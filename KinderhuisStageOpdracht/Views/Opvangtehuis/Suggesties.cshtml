@model KinderhuisStageOpdracht.Models.Viewmodels.OpvangtehuisViewModel.SuggestieListViewModel
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th class="suggestieColumns">Genre</th>
        <th class="suggestieColumns">Cliënt</th>
        <th class="suggestieColumns">Wanneer</th>
        <th>Suggestie</th>

    </tr>
    @foreach (var o in Model.Suggesties)
    {
        <tr>
            <td class="suggestieColumns">@Html.DisplayFor(modelItem => o.Genre)</td>
            <td class="suggestieColumns">@Html.DisplayFor(modelItem => o.Client)</td>
            <td class="suggestieColumns">@Html.DisplayFor(modelItem => o.TimeStamp)</td>
            <td class="omschrijvingColumn">@Html.DisplayFor(modelItem => o.Beschrijving)</td>
        </tr>
    }
</table>

@Html.ActionLink("Terug naar index", "OpvoederIndex", "Gebruiker")

